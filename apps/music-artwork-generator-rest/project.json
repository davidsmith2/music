{
  "name": "music-artwork-generator-rest",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/music-artwork-generator-rest/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/music-artwork-generator-rest",
        "main": "apps/music-artwork-generator-rest/src/main.ts",
        "tsConfig": "apps/music-artwork-generator-rest/tsconfig.app.json",
        "assets": ["apps/music-artwork-generator-rest/src/assets"],
        "target": "node",
        "compiler": "tsc",
        "webpackConfig": "apps/music-artwork-generator-rest/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/music-artwork-generator-rest/src/environments/environment.ts",
              "with": "apps/music-artwork-generator-rest/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "music-artwork-generator-rest:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/music-artwork-generator-rest"],
      "options": {
        "jestConfig": "apps/music-artwork-generator-rest/jest.config.ts"
      }
    }
  }
}
