{
  "name": "music-artwork-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/music-artwork-generator/src",
  "prefix": "music",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/music-artwork-generator",
        "index": "apps/music-artwork-generator/src/index.html",
        "main": "apps/music-artwork-generator/src/main.ts",
        "polyfills": "apps/music-artwork-generator/src/polyfills.ts",
        "tsConfig": "apps/music-artwork-generator/tsconfig.app.json",
        "assets": [
          "apps/music-artwork-generator/src/favicon.ico",
          "apps/music-artwork-generator/src/assets"
        ],
        "styles": ["apps/music-artwork-generator/src/styles.scss"],
        "scripts": [],
        "vendorChunk": true,
        "extractLicenses": false,
        "buildOptimizer": false,
        "sourceMap": true,
        "optimization": false,
        "namedChunks": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/music-artwork-generator/src/environments/environment.ts",
              "with": "apps/music-artwork-generator/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ]
        },
        "development": {}
      },
      "defaultConfiguration": ""
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "proxyConfig": "apps/music-artwork-generator/proxy.conf.json",
        "buildTarget": "music-artwork-generator:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "music-artwork-generator:build:production"
        },
        "development": {
          "buildTarget": "music-artwork-generator:build:development"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "music-artwork-generator:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/music-artwork-generator"],
      "options": {
        "jestConfig": "apps/music-artwork-generator/jest.config.ts"
      }
    }
  }
}
