{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "music-artwork-generator",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "api-interfaces": {
      "projectType": "library",
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "prefix": "davidsmith",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api-interfaces/src/**/*.ts",
              "libs/api-interfaces/src/**/*.html"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/api-interfaces"
          ],
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "music-artwork-generator": {
      "projectType": "application",
      "root": "apps/music-artwork-generator",
      "sourceRoot": "apps/music-artwork-generator/src",
      "prefix": "davidsmith",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/music-artwork-generator",
            "index": "apps/music-artwork-generator/src/index.html",
            "main": "apps/music-artwork-generator/src/main.ts",
            "polyfills": "apps/music-artwork-generator/src/polyfills.ts",
            "tsConfig": "apps/music-artwork-generator/tsconfig.app.json",
            "assets": [
              "apps/music-artwork-generator/src/favicon.ico",
              "apps/music-artwork-generator/src/assets"
            ],
            "styles": [
              "apps/music-artwork-generator/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/music-artwork-generator/src/environments/environment.ts",
                  "with": "apps/music-artwork-generator/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {}
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "music-artwork-generator:build",
            "proxyConfig": "apps/music-artwork-generator/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "music-artwork-generator:build:production"
            },
            "development": {
              "browserTarget": "music-artwork-generator:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "music-artwork-generator:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/music-artwork-generator/src/**/*.ts",
              "apps/music-artwork-generator/src/**/*.html"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/music-artwork-generator"
          ],
          "options": {
            "jestConfig": "apps/music-artwork-generator/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "music-artwork-generator-api": {
      "root": "apps/music-artwork-generator-api",
      "sourceRoot": "apps/music-artwork-generator-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/music-artwork-generator-api",
            "main": "apps/music-artwork-generator-api/src/main.ts",
            "tsConfig": "apps/music-artwork-generator-api/tsconfig.app.json",
            "assets": [
              "apps/music-artwork-generator-api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/music-artwork-generator-api/src/environments/environment.ts",
                  "with": "apps/music-artwork-generator-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "music-artwork-generator-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/music-artwork-generator-api/**/*.ts"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/music-artwork-generator-api"
          ],
          "options": {
            "jestConfig": "apps/music-artwork-generator-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "music-artwork-generator-e2e": {
      "root": "apps/music-artwork-generator-e2e",
      "sourceRoot": "apps/music-artwork-generator-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/music-artwork-generator-e2e/cypress.json",
            "tsConfig": "apps/music-artwork-generator-e2e/tsconfig.e2e.json",
            "devServerTarget": "music-artwork-generator:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "music-artwork-generator:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/music-artwork-generator-e2e/**/*.{js,ts}"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        }
      }
    }
  }
}
